#define FASTLED_INTERNAL
#include <FastLED.h>

#define LED_PIN 5
#define NUM_LEDS 50

CRGB leds[NUM_LEDS];

void setup() {
  FastLED.addLeds<WS2812, LED_PIN, GRB>(leds, NUM_LEDS);
}

void loop() {
  
  
    leds[3] = CRGB(255, 255, 255);
      FastLED.show();
    delay(1000);
    leds[3] = CRGB(0, 0, 0);
  FastLED.show();
  delay(1000);
  
  
  leds[4] = CRGB(255, 255, 255);
      FastLED.show();
    delay(1000);
    leds[4] = CRGB(0, 0, 0);
  FastLED.show();
  delay(100);
  
  
  leds[5] = CRGB(255, 255, 255);
      FastLED.show();
    delay(1000);
    leds[5] = CRGB(0, 0, 0);
  FastLED.show();
  delay(100);
  
  
  leds[6] = CRGB(255, 255, 255);
      FastLED.show();
    delay(1000);
    leds[6] = CRGB(0, 0, 0);
  FastLED.show();
  delay(100);

  
}
